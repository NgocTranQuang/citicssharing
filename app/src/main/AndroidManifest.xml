        <activity android:name="com.citics.valuation.ui.activity.tracuu.ChiTietDuAnActivity" />
        <activity android:name="com.citics.valuation.ui.activity.tracuu.TimKiemCanHoActivity" />
        <activity android:name="com.citics.valuation.ui.activity.tracuu.ChiTietCanHoActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>